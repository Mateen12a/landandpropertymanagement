            <a href="/" class="logo m-0 float-start">Property Basket</a>

            <ul
              class="js-clone-nav d-none d-lg-inline-block text-start site-menu float-end"
            >
              <li class="<%= activePage === 'home' ? 'active' : '' %>"><a href="/">Home</a></li>
              <li class="has-children <%= activePage === 'properties' ? 'active' : '' %>">
                <a href="/properties">Properties</a>
                <ul class="dropdown">
                  <li><a href="/buy">Buy Property</a></li>
                  <li><a href="/sell">Sell Property</a></li>
                  <li class="has-children">
                    <a href="#">Account</a>
                    <ul class="dropdown">
                      <li><a href="/login">Login</a></li>
                      <li><a href="/register">Register</a></li>
                    </ul>
                  </li>
                </ul>
              </li>
              <li class="<%= activePage === 'services' ? 'active' : '' %>"><a href="/services">Services</a></li>
              <li class="<%= activePage === 'about' ? 'active' : '' %>"><a href="/about">About</a></li>
              <!-- <li class="<%= activePage === 'contact' ? 'active' : '' %>"><a href="/contact">Contact Us</a></li> -->
              <% if (activePage !== '404') { %>
              <% if (user) { %>
                <% if (user.role === 'agent' || user.role === 'admin') { %>
                  <li class="<%= activePage === 'newproperty' ? 'active' : '' %>"><a href="/property/new" class="nav__btn--cta"><i class="fa-solid fa-paper-plane"></i> Submit A Property</a></li>
                <% } %>
                <li class="<%= activePage === 'logout' ? 'active' : '' %>"><a href="javascript:void(0);" id="nav__btn--logout" class="nav__btn--logout"><i class="fa-solid fa-sign-out"></i> Logout</a></li>
                <li class="<%= activePage === 'bookmarks' ? 'active' : '' %>"><a href="/bookmarks" class="bookmarks__link"><i class="fa-solid fa-bookmark"></i> Bookmarks</a></li>
              <% } else { %>
                <li class="<%= activePage === 'register' ? 'active' : '' %>"><a class="nav__btn--cta" href="/register"><i class="fa-solid fa-user-plus"></i> Register</a></li>
                <li class="<%= activePage === 'login' ? 'active' : '' %>"><a href="/login"><i class="fa-solid fa-right-to-bracket"></i> Login</a></li>
              <% } %>
              <% } %>
            </ul>

            <a
              href="#"
              class="burger light me-auto float-end mt-1 site-menu-toggle js-menu-toggle d-inline-block d-lg-none"
              data-toggle="collapse"
              data-target="#main-navbar"
            >
              <span></span>
            </a>
          </div>
        </div>
      </div>
    </nav>

